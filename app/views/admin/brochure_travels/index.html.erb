<div class="row">
<h3>后台管理 > Brochure travel列表 <div class="pull-right"><%= link_to '创建新Brochure travels', new_admin_brochure_travel_path %></div></h3>
<hr>
<table class="table table-striped table-bordered"> 
  <tr>
    <th>ID</th>
    <th>Brochure</th>
    <th>Travel</th>
    <th>操作</th>
  </tr>

<% @brochure_travels.each do |brochure_travel| %>
  <tr>
    <td>
      <%= brochure_travel.id %>
    </td>
    <td><%= brochure_travel.brochure_id %></td>
    <td><%= brochure_travel.travel_id %></td>
    <td>
      <%= link_to '查看', [:admin, brochure_travel] %> | 
      <%= link_to '编辑', edit_admin_brochure_travel_path(brochure_travel) %> | 
      <%= link_to '删除', [:admin, brochure_travel], :confirm => '确定要删除么?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
<%= paginate @brochure_travels %>
</div>