<div class="row">
<h3>后台管理 > Brochure destination列表 <div class="pull-right"><%= link_to '创建新Brochure destinations', new_admin_brochure_destination_path %></div></h3>
<hr>
<table class="table table-striped table-bordered"> 
  <tr>
    <th>ID</th>
    <th>Brochure</th>
    <th>Destination</th>
    <th>操作</th>
  </tr>

<% @brochure_destinations.each do |brochure_destination| %>
  <tr>
    <td>
      <%= brochure_destination.id %>
    </td>
    <td><%= brochure_destination.brochure_id %></td>
    <td><%= brochure_destination.destination_id %></td>
    <td>
      <%= link_to '查看', [:admin, brochure_destination] %> | 
      <%= link_to '编辑', edit_admin_brochure_destination_path(brochure_destination) %> | 
      <%= link_to '删除', [:admin, brochure_destination], :confirm => '确定要删除么?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
<%= paginate @brochure_destinations %>
</div>