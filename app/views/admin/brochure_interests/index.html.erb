<div class="row">
<h3>后台管理 > Brochure interest列表 <div class="pull-right"><%= link_to '创建新Brochure interests', new_admin_brochure_interest_path %></div></h3>
<hr>
<table class="table table-striped table-bordered"> 
  <tr>
    <th>ID</th>
    <th>Brochure</th>
    <th>Interest</th>
    <th>操作</th>
  </tr>

<% @brochure_interests.each do |brochure_interest| %>
  <tr>
    <td>
      <%= brochure_interest.id %>
    </td>
    <td><%= brochure_interest.brochure_id %></td>
    <td><%= brochure_interest.interest_id %></td>
    <td>
      <%= link_to '查看', [:admin, brochure_interest] %> | 
      <%= link_to '编辑', edit_admin_brochure_interest_path(brochure_interest) %> | 
      <%= link_to '删除', [:admin, brochure_interest], :confirm => '确定要删除么?', :method => :delete %>
    </td>
  </tr>
<% end %>
</table>
<%= paginate @brochure_interests %>
</div>