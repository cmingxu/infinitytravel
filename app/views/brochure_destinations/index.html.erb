<h1>Listing brochure_destinations</h1>

<table>
  <tr>
    <th>Brochure</th>
    <th>Destination</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @brochure_destinations.each do |brochure_destination| %>
  <tr>
    <td><%= brochure_destination.brochure_id %></td>
    <td><%= brochure_destination.destination_id %></td>
    <td><%= link_to 'Show', brochure_destination %></td>
    <td><%= link_to 'Edit', edit_brochure_destination_path(brochure_destination) %></td>
    <td><%= link_to 'Destroy', brochure_destination, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

