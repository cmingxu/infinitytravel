<div class="self_clear">
	<div id="secondary_content" class="hide_from_print">
		<div id="cs_control_3131" class="">
			<div class="CS_Element_Schedule" title="" id="CS_Element_contentleft">
				<a name="custom12103" id="custom12103"></a>
				<a name="CP_JUMP_12103" id="CP_JUMP_12103"></a>
				<div id="cs_control_12103" class="cs_control CS_Element_Custom">
					<div class="hl3">旅行导航</div>
					<!-- OHSU Nav -->
					<ul class="section cspot_nav">
						<% Interest.all.each do |interest| %>
						<li>
							<a href="search/" title="<%= interest.title %>"><%= interest.title %></a>
